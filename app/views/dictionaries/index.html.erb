<h1>Listing dictionaries</h1>

<table>
  <tr>
    <th>Title</th>
    <th>Content</th>
    <th>Descriptin</th>
    <th>User</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @dictionaries.each do |dictionary| %>
  <tr>
    <td><%= dictionary.title %></td>
    <td><%= dictionary.content %></td>
    <td><%= dictionary.descriptin %></td>
    <td><%= dictionary.user_id %></td>
    <td><%= link_to 'Show', dictionary %></td>
    <td><%= link_to 'Edit', edit_dictionary_path(dictionary) %></td>
    <td><%= link_to 'Destroy', dictionary, confirm: 'Are you sure?', method: :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Dictionary', new_dictionary_path %>
